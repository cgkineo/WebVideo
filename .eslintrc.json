{
    "env": {
        "browser": true,
        "node": true,
        "commonjs": false,
        "es2020": true
    },
    "extends": [
        "standard",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "react/prop-types": "off"
    },
    "settings": {
        "react": {
            "version": "17.02"
        }
    }
}
